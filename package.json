{
  "name": "vue-birth-datepicker",
  "description": "Date picker for distant dates, such as birthday. Just three clicks to choose any month and day 20 or 50 years ago.",
  "version": "0.1.0",
  "private": false,
  "main":   "dist/vueBirthDatepicker.common.js",
  "module": "dist/vueBirthDatepicker.umd.js",
  "unpkg":  "dist/vueBirthDatepicker.umd.min.js",
  "browser": {
    "./vue": "src/components/birth-datepicker.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-comp": "vue-cli-service build --target lib --name vueBirthDatepicker src/components/birth-datepicker.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
